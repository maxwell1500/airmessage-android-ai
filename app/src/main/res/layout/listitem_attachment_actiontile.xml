<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="@dimen/contenttile_size"
	android:layout_height="@dimen/contenttile_size"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_marginEnd="@dimen/contenttile_margin"
	android:background="@drawable/rounded_selectable_contenttile"
	android:backgroundTint="@color/colorToolbar"
	android:padding="10dp">
	
	<TextView
		android:id="@+id/label"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		app:drawableTint="?android:attr/textColorSecondary"
		android:drawableTop="@drawable/add"
		android:gravity="center"
		android:text="@string/part_unknown"
		android:textColor="?android:attr/textColorSecondary" />
</FrameLayout>